    <script>
        document.addEventListener('DOMContentLoaded', function () {
            // Get references to the cards
            var filterCard = document.getElementById('filterCard');
            var displayFieldsCard = document.getElementById('displayFieldsCard');

            // Function to toggle both cards
            function toggleCards() {
                // Check if the filter card is collapsed
                var isCollapsed = filterCard.classList.contains('collapsed-card');

                // Toggle the classes for both cards
                filterCard.classList.toggle('collapsed-card', !isCollapsed);
                displayFieldsCard.classList.toggle('collapsed-card', !isCollapsed);

                // Optionally, add a transition effect
                filterCard.style.transition = 'max-height 0.5s ease-in-out';
                displayFieldsCard.style.transition = 'max-height 0.5s ease-in-out';
            }

            // Add event listener to the filter card header
            filterCard.querySelector('.card-header').addEventListener('click', toggleCards);

            // Initially set the state to collapsed
            toggleCards(); // This line ensures both cards start in the collapsed state
        });
    </script>
    <script>
        var preventBack = @Html.Raw(Json.Serialize(ViewData["PreventBack"] ?? false));

        if (preventBack) {
            window.history.pushState(null, "", location.href);
            window.onpopstate = function () {
                window.history.pushState(null, "", location.href);
            };
        }
    </script>
